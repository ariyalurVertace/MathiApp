{
    "name": "com.vertace.policeapp",
    "version": "0.1.0",
    "private": true,
    "main": "index.js",
    "lint-staged": {
        "*.js": [
            "prettier --ignore-path .gitignore --write --tab-width 4",
            "git add"
        ],
        "*.{json,md,scss,yaml,yml}": [
            "prettier --ignore-path .gitignore --write --tab-width 4",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "scripts": {
        "android-linux": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/",
        "pretty": "./node_modules/.bin/prettier --ignore-path .gitignore --write --tab-width 4 \"**/*.js\"",
        "lint": "./node_modules/.bin/eslint . --ignore-path .gitignore --fix",
        "delete-removed-branches": "git fetch -p && for branch in `git branch -vv | grep ': gone]' | awk '{print $1}'`; do git branch -D $branch; done",
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "postinstall": "npx jetify"
    },
    "rnpm": {
        "assets": [
            "./fonts/"
        ]
    },
    "dependencies": {
